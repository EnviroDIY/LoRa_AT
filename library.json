{
  "name": "TinyLoRa",
  "version": "0.1.0",
  "description": "A small Arduino library for LoRa modules, that just works. Supports modules with AT command interfaces.",
  "keywords": "LoRa, LoRaWAN, AT commands, AT, mDOT",
  "authors": [
    {
      "name": "Sara Damiano",
      "url": "https://github.com/EnviroDIY",
      "maintainer": true
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EnviroDIY/TinyLoRa.git"
  },
  "homepage": "https://github.com/EnviroDIY/TinyLoRa",
  "export": {
    "include": [
      "LICENSE",
      "library.json",
      "library.properties",
      "README.md",
      "keywords.txt",
      "src/*",
      "examples/*"
    ]
  },
  "frameworks": ["arduino", "energia", "wiringpi"],
  "platforms": "*",
  "headers": ["TinyLoRa.h"],
  "build": {
    "libLDFMode": "deep+"
  }
}
