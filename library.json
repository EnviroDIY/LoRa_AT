{
  "name": "LoRa_AT",
  "version": "0.4.2",
  "description": "A small Arduino library for AT command based LoRa Modules",
  "keywords": "LoRa, LoRaWAN, AT commands, AT, mDOT, LoRaE5",
  "authors": [
    {
      "name": "Sara Damiano",
      "url": "https://github.com/EnviroDIY",
      "maintainer": true
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EnviroDIY/LoRa_AT.git"
  },
  "homepage": "https://github.com/EnviroDIY/LoRa_AT",
  "export": {
    "include": [
      "LICENSE",
      "library.json",
      "library.properties",
      "README.md",
      "keywords.txt",
      "src/*",
      "examples/*",
      "extras/*"
    ]
  },
  "frameworks": ["arduino", "energia", "wiringpi"],
  "platforms": "*",
  "headers": ["LoRa_AT.h"],
  "build": {
    "libLDFMode": "deep+"
  }
}
